<%= form_for (@article) do |f| %>
<ul>
<%@article.errors.full_messages.each do |error|%>
<li><%=error%></li>
<%end%>
</ul>

<%= f.label :title %>
<%= f.text_field :title %>


<%= f.label :body %>
<%= f.text_area :body %>

<%= f.submit "+", class: "btn-floating btn-large waves-effect waves-light red submit"%>
<%# <a class="btn-floating btn-large waves-effect waves-light red"></a> %>


<% end %>